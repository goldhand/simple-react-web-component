{
  "name": "simple-react-web-component",
  "homepage": "https://www.willfarley.org/simple-react-web-component",
  "version": "0.1.0",
  "description": "A simple POC rendering react components as web components",
  "main": "dist",
  "scripts": {
    "start": "webpack-dev-server --mode development --config webpack.website.config.js",
    "package": "webpack --mode production",
    "clean": "rm -rf ./dist/*",
    "prepublishOnly": "npm run clean && npm install && npm run package",
    "build": "webpack --mode production --config webpack.website.config.js && cp -r ./public/* ./build/",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "keywords": [],
  "author": "Will Farley <a.will.farley@gmail.com> (http://www.willfarley.org/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "eslint": "^5.7.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.1",
    "ramda": "^0.25.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  }
}
